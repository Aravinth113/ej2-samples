{"index.ts":"/**\n *  Toolbar sample to demonstrate default functionalities.\n */\nimport { Toolbar } from '@syncfusion/ej2-navigations';\n\n\n    let ele: string = '<div class= \"e-folder\"><div class = \"e-folder-name\">Inbox(33)</div>';\n    ele = ele +  '<div class =\"e-mail-id\">user@example.com</div></div> ';\n    let toolbarObj: Toolbar = new Toolbar({\n        items: [\n            {\n              prefixIcon: 'e-tbar-menu-icon tb-icons', tooltipText: 'Menu' },\n            {\n              template: ele,\n              align: 'center' },\n            {\n              prefixIcon: 'e-tbar-search-icon tb-icons', tooltipText: 'Search', align: 'right' },\n            {\n              prefixIcon: 'e-tbar-settings-icon tb-icons', tooltipText: 'Popup', align: 'right' } ]\n    });\n    toolbarObj.appendTo('#toolbar_align');\n\n","index.html":"<html><head>\n    <link href=\"http://ej2ci.syncfusion.com/production/demos/styles/material.css\" rel=\"stylesheet\">\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/systemjs/0.19.38/system.js\"></script>\n    <script src=\"systemjs.config.js\"></script>\n</head>\n<body>\n<style>\n    /* Toolbar Styles */\n\n    @font-face {\n        font-family: 'Material_toolbar';\n        src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');\n        font-weight: normal;\n        font-style: normal;\n    }\n\n    .e-bigger .e-tbar-btn .tb-icons {\n        font-size: 20px;\n    }\n\n    @media screen and (max-width: 768px) {\n     .e-toolbar .e-toolbar-item .e-icons:after {\n        line-height: 2.3;\n        content: '';\n       }     \n     }\n\n    .e-toolbar-item .e-icons:after {\n      line-height: 2;\n      content: '';\n    }\n    .e-tbar-btn .tb-icons {\n        font-family: 'Material_toolbar';\n        speak: none;\n        font-size: 16px;\n        font-style: normal;\n        font-weight: normal;\n        font-variant: normal;\n        text-transform: none;\n    }\n\n    .e-toolbar .e-icons {\n        font-size: 20px;\n    }\n\n    .e-tbar-menu-icon:before {\n        content: \"\\e718\";\n    }\n\n    .e-tbar-search-icon:before {\n        content: \"\\e71d\";\n    }\n\n    .e-tbar-settings-icon:before {\n        content: \"\\e702\";\n    }\n\n    .control-wrapper {\n        flex-direction: inherit\n    }\n\n    .e-sample-resize-container {\n        height: 42px;\n    }\n\n    .e-def-avator {\n        margin: 8px;\n        border-radius: 50%;\n        width: 40px;\n        font-size: 20px;\n        border: 2px solid #666;\n    }\n\n    .e-def-avator span {\n        text-align: center;\n        line-height: 40px;\n        display: block;\n    }\n\n    .e-mail-header,\n    .e-mail-subject,\n    .e-mail-description,\n    .e-mail-id {\n        white-space: nowrap;\n        overflow: hidden;\n        text-overflow: ellipsis;\n    }\n\n    .e-folder {\n        text-align: center;\n    }\n\n    .e-mail-header,\n    .e-mail-subject,\n    .e-folder {\n        font-weight: 500;\n        font-style: normal;\n    }\n\n    .e-mail-item {\n        margin-top: 8px;\n        border-bottom: 1px solid #d3d3d3;\n    }\n\n    .e-mail-image {\n        margin-right: 10px;\n        float: left;\n        height: 60px;\n    }\n</style>\n<div class=\"col-lg-12 control-section\">\n    <div class=\"e-sample-resize-container\">\n        <div id=\"toolbar_align\">\n        </div>\n        <div class=\"e-mail-items\">\n            <div class=\"e-mail-item\">\n                <div class=\"e-mail-image\">\n                    <div class=\"e-def-avator\"><span>MA</span></div>\n                </div>\n                <div class=\"e-mail-content\"><span class=\"e-mail-header\">Maria Anders</span><span class=\"e-mail-time\" style=\"float: right\">11:27AM</span>\n                    <div class=\"e-mail-subject\"> Sales Representative </div>\n                <div class=\"e-mail-description\"> Can we schedule Meeting Appointment for today? </div>\n            </div>\n        </div>\n        <div class=\"e-mail-item\">\n            <div class=\"e-mail-image\">\n                <div class=\"e-def-avator\"><span>VA</span></div>\n            </div>\n            <div class=\"e-mail-content\"><span class=\"e-mail-header\">Victoria Ashworth</span><span class=\"e-mail-time\" style=\"float: right\">Fri 7:50AM</span>\n                <div class=\"e-mail-subject\"> Sales Representative </div>\n            <div class=\"e-mail-description\"> Yes we are available for meeting tomorrow </div>\n        </div>\n    </div>\n    <div class=\"e-mail-item\">\n        <div class=\"e-mail-image\">\n            <div class=\"e-def-avator\"><span>TH</span></div>\n        </div>\n        <div class=\"e-mail-content\"><span class=\"e-mail-header\">Thomas Hardey</span><span class=\"e-mail-time\" style=\"float: right\">Fri 7:50AM</span>\n            <div class=\"e-mail-subject\"> Sales Representative </div>\n        <div class=\"e-mail-description\"> Customer has accepted our proposal. Would it be possible for arrange meeting tomorrow? </div>\n    </div>\n</div>\n</div>\n</div>\n</div>\n\n\n</body></html>","systemjs.config.js":"System.config({\n    transpiler: \"typescript\",\n    typescriptOptions: {\n        compilerOptions: {\n            target: \"umd\",\n            module: \"commonjs\",\n            moduleResolution: \"node\",\n            emitDecoratorMetadata: true,\n            experimentalDecorators: true\n        }\n    },\n    paths: {\n        \"syncfusion:\": \"http://ej2ci.syncfusion.com/packages/production/\"\n    },\n    map: {\n        main: \"index.ts\",\n        typescript: \"https://unpkg.com/typescript@2.2.2/lib/typescript.js\",\n        'plugin-json':'https://cdnjs.cloudflare.com/ajax/libs/systemjs-plugin-json/0.3.0/json.min.js',\n        \"@syncfusion/ej2-base\": \"syncfusion:ej2-base/dist/ej2-base.umd.min.js\",\n        \"@syncfusion/ej2-buttons\": \"syncfusion:ej2-buttons/dist/ej2-buttons.umd.min.js\",\n        \"@syncfusion/ej2-calendars\": \"syncfusion:ej2-calendars/dist/ej2-calendars.umd.min.js\",\n        \"@syncfusion/ej2-charts\": \"syncfusion:ej2-charts/dist/ej2-charts.umd.min.js\",\n        \"@syncfusion/ej2-data\": \"syncfusion:ej2-data/dist/ej2-data.umd.min.js\",\n        \"@syncfusion/ej2-grids\": \"syncfusion:ej2-grids/dist/ej2-grids.umd.min.js\",        \n        \"@syncfusion/ej2-inputs\": \"syncfusion:ej2-inputs/dist/ej2-inputs.umd.min.js\",        \n        \"@syncfusion/ej2-lists\": \"syncfusion:ej2-lists/dist/ej2-lists.umd.min.js\",\n        \"@syncfusion/ej2-navigations\": \"syncfusion:ej2-navigations/dist/ej2-navigations.umd.min.js\",        \n        \"@syncfusion/ej2-popups\": \"syncfusion:ej2-popups/dist/ej2-popups.umd.min.js\",  \n        \"@syncfusion/ej2-circulargauge\": \"syncfusion:ej2-circulargauge/dist/ej2-circulargauge.umd.min.js\",      \n    },\n     meta: { \n       '*.json': { loader: 'plugin-json' }\n    }\n});\n\nSystem.import('index.ts').catch(console.error.bind(console));"}